<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
	<bean id="itd-share-extension-registry-messages-deployer" class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent" depends-on="">
		<property name="resourceBundles">
			<list>
				<value>alfresco.web-extension.messages.itd-extension-registry</value>
			</list>
		</property>
	</bean>
	<bean id="itd-share-extension-registry" class="ru.itdsystems.alfresco.ShareExtensionRegistry" />
	<bean id="itd-share-extension-registry-js" class="ru.itdsystems.alfresco.jscript.JSShareExtensionRegistry" depends-on="itd-share-extension-registry" parent="baseScriptExtension">
		<property name="extensionName">
			<value>extensionRegistry</value>
		</property>
		<property  name="shareExtensionRegistry" ref="itd-share-extension-registry"/>
	</bean>
</beans>
