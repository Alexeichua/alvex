<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
	<bean id="alvex-orgchart-md" parent="dictionaryModelBootstrap"
		depends-on="dictionaryBootstrap">
		<!-- Register resource bundles -->
		<property name="labels">
			<list>
				<value>alfresco.extension.messages.alvex-orgchart</value>
			</list>
		</property>
		<!-- Register models -->
		<property name="models">
			<list>
				<value>alfresco/extension/models/alvex-orgchart-content-model.xml</value>
			</list>
		</property>
	</bean>
	<bean id="alvex-orgchart-service" class="com.alvexcore.repo.orgchart.OrgchartService">
		<property name="authorityService" ref="AuthorityService" />
		<property name="nodeService" ref="NodeService" />
	</bean>
	<bean id="alvex-orgchart-js" parent="baseJavaScriptExtension"
		class="com.alvexcore.repo.orgchart.jscript.JscriptOrgchart">
		<property name="extensionName">
			<value>orgchart</value>
		</property>
		<property name="orgchartService" ref="alvex-orgchart-service" />
		<property name="serviceRegistry" ref="ServiceRegistry" />
	</bean>
	<bean id="alvex-orgchart" parent="alvex-extension"
		class="com.alvexcore.repo.OrgchartExtension" depends-on="alvex-orgchart-md,alvex-orgchart-js">
		<property name="orgchartService" ref="alvex-orgchart-service" />
	</bean>
</beans>
