+/*
+ * То, где перед URL стоит * - можно пока не делать
+ * Точные параметры POST /api/alvex/orgchart/{branch}/{unit} я еще не понял, это за мной.
+ * Ко всем возвращаемым объектам нужен NodeRef, потому что дальше любые запросы к API Alfresco на него завязаны.
+ * shortName считается синоним ID
+ */
+
+/*
+ * Работа с ветками оргструктуры
+ */
+
+GET /api/alvex/orgchart/branches
+// IN -
+// OUT - список имен бранчей
+
+PUT /api/alvex/orgchart/branches/{branch}
+// IN - 
+// OUT - детали бранче
+
+* DELETE /api/alvex/orgchart/branches/{branch}
+// IN - 
+// OUT - 


+/*
+ * Работа с определениями ролей
+ */
+
+GET /api/alvex/orgchart/role-definitions
+// IN - 
+// OUT - список дефинишнов { shortName, displayName, weight }
+
+PUT /api/alvex/orgchart/role-definitions
+// IN - { displayName, weight }
+// OUT - детали дефинишна
+
+DELETE /api/alvex/orgchart/role-definitions/{role}
+// IN - 
+// OUT - 
+
+POST /api/alvex/orgchart/role-definitions/{role}
+// IN - { newDisplayName, newWeight }
+// OUT - детали дефинишна


/*
 * Работа с деревом юнитов
 */

+GET /api/alvex/orgchart/tree/{branch}
+// IN - 
+// OUT - дерево юнитов { shortName, displayName, children: [ ... ] }
+
+GET /api/alvex/orgchart/units/{id}
+// IN - 
+// OUT - список людей в юните { shortName, displayName, roleShortName, roleDisplayName }
+// 		Если человек в N ролях - присутствует в ответе N раз
+// 		Если человек просто в members - в качестве roleShortName '', в качестве roleDisplayName 'members'

+PUT /api/alvex/orgchart/units/{id}
+// IN - { displayName, weight, name }
+// OUT - unit details
+
+DELETE /api/alvex/orgchart/units/{id}
+// IN - 
+// OUT - 
+
+POST /api/alvex/orgchart/{branch}/{unit}
+// IN - displayName, weight
+// OUT - 
+
+
+/*
+ * Работа с членами юнитов
+ */
+
+GET /api/alvex/orgchart/units/{id}/members
+// IN - 
+// OUT - список участников { shortName, displayName }
+
+PUT /api/alvex/orgchart/units/{id}/members
+// IN - список логинов { shortName }
+// OUT - 
+
+DELETE /api/alvex/orgchart/units/{id}/members
+// IN - список логинов { shortName }
+// OUT - 
+
+
+/*
+ * Работа с правами на юниты
+ */
+
+* GET /api/alvex/orgchart/units/{id}/admins
+// IN - 
+// OUT - список участников { shortName, displayName }
+
+* PUT /api/alvex/orgchart/units/{id}/admins
+// IN - список логинов { shortName }
+// OUT - 
+
+* DELETE /api/alvex/orgchart/units/{id}/admins
+// IN - список логинов { shortName }
+// OUT - 
+
+GET /api/alvex/orgchart/units/{id}/supervisors
+// IN - 
+// OUT - список участников { shortName, displayName }
+
+PUT /api/alvex/orgchart/units/{id}/supervisors
+// IN - список логинов { shortName }
+// OUT - 
+
+DELETE /api/alvex/orgchart/units/{id}/supervisors
+// IN - список логинов { shortName }
+// OUT - 
+
+
+/*
+ * Работа с ролями юнитов
+ */
+
+GET /api/alvex/orgchart/units/{id}/roles
+// IN - 
+// OUT - список ролей { shortName, displayName }
+
+PUT /api/alvex/orgchart/units/{id}/roles
+// IN - список ролей { shortName }
+// OUT - 
+
+DELETE /api/alvex/orgchart/units/{id}/roles
+// IN - список ролей { shortName }
+// OUT - 
+
+GET /api/alvex/orgchart/units/{id}/roles/{role}/members
+// IN - список участников { shortName, displayName }
+// OUT - 
+
+PUT /api/alvex/orgchart/units/{id}/roles/{role}/members
+// IN - список логинов { shortName }
+// OUT - 
+
+DELETE /api/alvex/orgchart/{units}/{id}/roles/{role}/members
+// IN - список логинов { shortName }
+// OUT - 
+
+
+/*
+ * Специализированные API
+ */
+
+GET /api/alvex/orgchart/managees/{user}
+// IN - 
+// OUT - [  { unitShortName, unitDisplayName, managees: [ { shortName, displayName }, ... ] } , ... ]
