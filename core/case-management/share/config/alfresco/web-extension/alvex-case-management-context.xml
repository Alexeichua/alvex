<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
	<bean id="alvex-case-management-messages-deployer" class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent" depends-on="">
		<property name="resourceBundles">
			 <list>
				<value>alfresco.web-extension.messages.alvex-case-management</value>
			</list>
		</property>
	</bean>
	<bean id="alvex-case-management-cd" class="org.springframework.extensions.config.ConfigBootstrap"
		init-method="register" depends-on="">
		<property name="configService" ref="web.config" />
		<property name="configs">
			<list>
				<value>classpath:alfresco/web-extension/alvex-case-management-config.xml</value>
			</list>
		</property>
	</bean>
	<bean id="alvex-case-management" class="com.alvexcore.share.CaseManagementExtension" parent="alvex-extension"/>
</beans>
